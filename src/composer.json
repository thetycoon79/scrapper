{
    "name": "zulfadzly/zulfadzly",
    "type": "project",
    "description": "property listing sample",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "adhocore/cli": "^0.9.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "monolog/monolog": "^2.3",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
